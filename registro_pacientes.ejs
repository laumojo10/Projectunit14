<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <link href="https://fonts.googleapis.com/css2?family=Hanken+Grotesk:wght@500;700;800&family=Roboto:wght@100&display=swap" rel="stylesheet">
    <title>Registro de Pacientes</title>
</head>
<body style="background:linear-gradient(to bottom, hsl(252, 39%, 64%),  hsl(242, 50%, 44%));height: 900.833px;">
    <h1>Registro de Pacientes</h1>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Cedula</th>
            <th>Edad</th>
            <th>Telefono</th>
            <th>Especialidad</th>
        </tr>
        <% pacientes.forEach(function(paciente) { %>
            <tr>
                <td><%= paciente.nombre %></td>
                <td><%= paciente.apellido %></td>
                <td><%= paciente.cedula %></td>
                <td><%= paciente.edad %></td>
                <td><%= paciente.telefono %></td>
                <td><%= paciente.especialidad %></td>
                
        <% }) %>
    </table>
</body>
</html>